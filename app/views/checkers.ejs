<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Dynamic Templates</title>
    <link rel="stylesheet" href="/css/style.css">
  </head>
  <body>
    <div>
      <h1>hello</h1>
      <table>
        <% var bg, getsPiece = false, url %>
        <% for(var i = 1; i < 9; i++) { %>
          <tr class="tr<%= i % 2 %>">
            <% for(var c = 1; c < 9; c++) { %>
            <% if(i%2===1){ %>
              <% bg = (c%2===1) ? 'white' : 'black' %>
            <% }else{ %>
              <% bg = (c%2===1) ? 'black' : 'white' %>
            <% } %>
            <td class="td<%= c%2 %>"></td>
            <% } %>
          </tr>
        <% } %>
      </table>
    </div>
  </body>
</html>
